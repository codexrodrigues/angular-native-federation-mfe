{
  "name": "remote-gde",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "install:legacy": "npm install --legacy-peer-deps",
    "serve:dist": "http-server dist/remote-gde/browser -p 4202 --cors -c-1",
    "start": "ng serve",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "patch-package"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "es-module-shims": "^2.0.0",
    "rxjs": "~7.8.0",
    "shared-logic": "file:../releases/shared-logic.tgz",
    "shared-ui-lib": "file:../shared-ui-lib/dist",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^20.1.7",
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "esbuild": "^0.27.2",
    "@types/node": "^22.0.0",
    "http-server": "^14.1.1",
    "jsdom": "^27.1.0",
    "patch-package": "^8.0.0",
    "sass": "^1.77.6",
    "typescript": "~5.8.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.54.0"
  }
}
